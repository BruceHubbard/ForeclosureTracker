<!DOCTYPE html>
<html>
<head>
  <title>Foreclosure</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= stylesheet_link_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/themes/ui-lightness/jquery-ui.css" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="navbar navbar-fixed-top">
		<div class="navbar-inner">
		      <div class="container">
		        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
		          <span class="icon-bar"></span>
		          <span class="icon-bar"></span>
		          <span class="icon-bar"></span>
		        </a>
		        <a class="brand" href="#">Sheriff Sale Tracker</a>
				<div class="pull-left">
					<div class="num-matches"></div>
				</div>
				<div class="pull-right">
					<div class="filters">
						<div class="appraised-range range filter">
							<h3>Appraised Value</h3>
							<label class="min">0</label>
							<span class="slider"></span>
							<label class="max">500,000</label>
						</div>
					</div>
				</div>
		        <div class="nav-collapse">
		        </div><!-- /.nav-collapse -->
		      </div>
		    </div>	</div>
<%= yield %>

</body>
</html>
